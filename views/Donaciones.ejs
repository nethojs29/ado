<!DOCTYPE html>
<html>
    <head>
        <title><%=title%></title>
        <script type="text/javascript" src="/../public/js/jquery-1.10.2.min.js"></script>
        <link rel="stylesheet" href="/../public/css/style.css">
    </head>
<body>
<div class="page-data">

    <form method="post" action="" id="the-form">
        <table cellpadding="11">
        <tr>
            <td class="label">Numero Contraloria:</td><td>: <input type="text" name="id_bien"></td>
         </tr>
         <tr>
            <td class="label"></td>
            <td>
                &nbsp; <input type="button" value="Tramitar" onClick="crearTramite()">
            </td>
         </tr>
        </table>
        <p>
    </form>
</div>


<div class="page-data">
     <ul class="err-area"></ul>
</div>

<script>

  function crearTramite() {
    console.log($("#the-form").serialize());
    $.ajax({

     url:"/api/Tramite/",
     type:"post",
     data:$("#the-form").serialize()+"&tipo=donacion",
     success:function(res){

         window.location.reload();
         return false;
     },
     error:function(xhr, status, error){
         console.log(xhr.responseText);
         var err = '';
         $.each(JSON.parse(xhr.responseText) , function(i, item) {

              err +='<li>'+item.msg+'</li>';
         });
         $(".err-area").html(err);
         return false;
     }

    });
  }

</script>
</body>
</html>
